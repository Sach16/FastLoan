<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/cardColor"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/rel_lay_bg2">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/LEADS_TXT_NO"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="bottom"
                android:layout_marginLeft="15dp"
                android:text="0"
                android:textColor="@color/buttonBg"
                android:textSize="36sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Leads"
                android:layout_marginLeft="5dp"
                android:textColor="@android:color/white"
                android:textStyle="bold"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/LEADS_AMT_NO"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="\u20B9 0"
                android:layout_marginLeft="15dp"
                android:textColor="@color/imgLineColor"
                android:textStyle="bold"
                android:textSize="14sp" />

        </LinearLayout>

        <ImageView
            android:id="@+id/GO_ARROW_LEADS"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:src="@drawable/arrow_icon" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/rel_lay_bg2">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/LOGINS_TXT_NO"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="bottom"
                android:layout_marginLeft="15dp"
                android:text="0"
                android:textColor="@color/buttonBg"
                android:textSize="36sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Logins"
                android:layout_marginLeft="5dp"
                android:textColor="@android:color/white"
                android:textStyle="bold"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/LOGINS_AMT_NO"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="\u20B9 0"
                android:layout_marginLeft="15dp"
                android:textColor="@color/imgLineColor"
                android:textStyle="bold"
                android:textSize="14sp" />

        </LinearLayout>

        <ImageView
            android:id="@+id/GO_ARROW_LOGINS"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:src="@drawable/arrow_icon" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/rel_lay_bg2">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/LOGINS_TXT_FINAL"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="bottom"
                android:layout_marginLeft="15dp"
                android:text="0"
                android:textColor="@color/buttonBg"
                android:textSize="36sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="First Disbursements"
                android:layout_marginLeft="5dp"
                android:textColor="@android:color/white"
                android:textStyle="bold"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/LOGINS_AMT_FINAL"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="\u20B9 0"
                android:layout_marginLeft="15dp"
                android:textColor="@color/imgLineColor"
                android:textStyle="bold"
                android:textSize="14sp" />

        </LinearLayout>

        <ImageView
            android:id="@+id/GO_ARROW_FINAL"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:src="@drawable/arrow_icon" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/rel_lay_bg2">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/LOGINS_TXT_PART"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="bottom"
                android:layout_marginLeft="15dp"
                android:text="0"
                android:textColor="@color/buttonBg"
                android:textSize="36sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Part Disbursements"
                android:layout_marginLeft="5dp"
                android:textColor="@android:color/white"
                android:textStyle="bold"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/LOGINS_AMT_PART"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="\u20B9 0"
                android:layout_marginLeft="15dp"
                android:textColor="@color/imgLineColor"
                android:textStyle="bold"
                android:textSize="14sp" />

        </LinearLayout>

        <ImageView
            android:id="@+id/GO_ARROW_PART"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:src="@drawable/arrow_icon" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/rel_lay_bg2">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/LOGINS_TXT_FULL"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="bottom"
                android:layout_marginLeft="15dp"
                android:text="0"
                android:textColor="@color/buttonBg"
                android:textSize="36sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Full Disbursements"
                android:layout_marginLeft="5dp"
                android:textColor="@android:color/white"
                android:textStyle="bold"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/LOGINS_AMT_FULL"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="\u20B9 0"
                android:layout_marginLeft="15dp"
                android:textColor="@color/imgLineColor"
                android:textStyle="bold"
                android:textSize="14sp" />

        </LinearLayout>

        <ImageView
            android:id="@+id/GO_ARROW_FULL"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:src="@drawable/arrow_icon" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/rel_lay_bg2">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/LEADS_TXT_PAYOUT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="bottom"
                android:layout_marginLeft="15dp"
                android:text="0"
                android:textColor="@color/buttonBg"
                android:textSize="36sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Builder Payout"
                android:layout_marginLeft="5dp"
                android:textColor="@android:color/white"
                android:textStyle="bold"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/LEADS_AMT_PAYOUT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="\u20B9 0"
                android:layout_marginLeft="15dp"
                android:textColor="@color/imgLineColor"
                android:textStyle="bold"
                android:textSize="14sp" />

        </LinearLayout>

    </RelativeLayout>

</LinearLayout>